{
  "plugins": {
    "@sounisi5011/metalsmith-netlify-published-date": {
      "pattern": "**/*.pug",
      "cacheDir": "./node_modules/.cache/",
      "filename2urlPath": {
        "metadata": "canonicalPath"
      },
      "plugins": {
        "metalsmith-in-place": {
          "setFilename": true
        }
      },
      "metadataUpdater": "./set-datetime",
      "contentsConverter": "./remove-time-elem"
    }
  }
}
